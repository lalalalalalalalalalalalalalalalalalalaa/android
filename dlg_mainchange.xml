<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/returnOrder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/return_oder" />

    <Button
        android:id="@+id/dlg_checkout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/dlg_checkout"/>

    <Button
        android:id="@+id/dlg_form"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/dlg_form" />

    <Button
        android:id="@+id/dlg_end"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/dlg_end" />

</LinearLayout>
